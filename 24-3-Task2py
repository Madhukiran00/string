str="Practice makes perfect"
vowels_count=0
consonants_count=0
c2=0
str=str.lower()
vowels="aeiou"
for i in range(0,len(str),1):
    ch=str[i]
    if vowels.find(ch)!=-1:
        vowels_count+=1
    else:
        consonants_count+=1
print(f" count of consonants is :{consonants_count}")
print(f" count of vowels is :{vowels_count}")

    

    
     